<p>Here are the reviews for <%= @instructor.name %>.  </p>

<ol>
    <li>
        <% @instructor.course_sessions.each do |session| %>
            <%= session.term %>
            <ol>
                <% session.reviews.each do |review| %>
                    <li> <%= review.score %> - <%= review.content %></li>
                <% end %>
            </ol>
        <% end %>
    </li>
</ol>
